{
  "name": "anything",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "update-packages": "turbo run update-packages"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "npm@9.8.1",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18"
  }
}
