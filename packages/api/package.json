{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "node watch.mjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist .build .esbuild",
    "db": "run-z db-dir db-install db-setup",
    "db-dir": "mkdir -p ../../docker/dynamodb",
    "db-install": "AWS_PROFILE=${AWS_PROFILE} sls dynamodb install",
    "db-setup": "AWS_PROFILE=${AWS_PROFILE} sls dynamodb start --migrate",
    "db-clean": "run-z db-remove-files db-uninstall",
    "db-remove-files": "rm -r ../../docker/dynamodb",
    "db-uninstall": "AWS_PROFILE=${AWS_PROFILE} sls dynamodb remove",
    "dev": "AWS_PROFILE=${AWS_PROFILE} AWS_DEFAULT_PROFILE=${AWS_PROFILE} sls offline start -c serverless.yml",
    "sls-deploy": "AWS_PROFILE=${AWS_PROFILE} sls deploy",
    "sls-deploy-fn": "AWS_PROFILE=${AWS_PROFILE} sls deploy function --function $*",
    "sls-dev": "AWS_PROFILE=${AWS_PROFILE} sls dev --verbose",
    "sls-remove": "AWS_PROFILE=${AWS_PROFILE} sls remove",
    "sls-deploy-local": "AWS_PROFILE=${AWS_PROFILE} sls deploy --stage local",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles",
    "update-packages": "ncu -u"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "@middy/core": "^5.3.2",
    "@middy/error-logger": "^5.3.2",
    "@middy/http-error-handler": "^5.3.2",
    "@middy/http-event-normalizer": "^5.3.2",
    "@middy/http-header-normalizer": "^5.3.2",
    "@middy/http-json-body-parser": "^5.3.2",
    "@middy/http-urlencode-path-parser": "^5.3.2",
    "@middy/validator": "^5.3.2",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "logger": "*",
    "utils": "*"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.540.0",
    "@aws-sdk/credential-providers": "^3.540.0",
    "@aws-sdk/lib-dynamodb": "^3.540.0",
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "esbuild": "^0.20",
    "eslint": "^8.57.0",
    "eslint-config-custom-server": "*",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jest-presets": "*",
    "npm-check-updates": "^16.14.17",
    "run-z": "^2.0.0",
    "serverless": "^3.38.0",
    "serverless-better-credentials": "^2.0.0",
    "serverless-dynamodb": "^0.2.51",
    "serverless-esbuild": "^1.52.1",
    "serverless-localstack": "^1.2.0",
    "serverless-offline": "^13.3.3",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-webpack": "^5.13.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig": "*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "resolutions": {
    "fast-xml-parser": ">=4.2.4",
    "semver": ">=7.5.2"
  }
}
