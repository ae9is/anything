{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "node watch.mjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist .build .esbuild",
    "db": "run-z db-dir db-install db-setup",
    "db-dir": "mkdir -p ../../docker/dynamodb",
    "db-install": "AWS_PROFILE=${AWS_PROFILE} sls dynamodb install",
    "db-setup": "AWS_PROFILE=${AWS_PROFILE} sls dynamodb start --migrate",
    "db-clean": "run-z db-remove-files db-uninstall",
    "db-remove-files": "rm -r ../../docker/dynamodb",
    "db-uninstall": "AWS_PROFILE=${AWS_PROFILE} sls dynamodb remove",
    "dev": "AWS_PROFILE=${AWS_PROFILE} AWS_DEFAULT_PROFILE=${AWS_PROFILE} sls offline start -c serverless.yml",
    "sls-deploy": "AWS_PROFILE=${AWS_PROFILE} sls deploy",
    "sls-deploy-fn": "AWS_PROFILE=${AWS_PROFILE} sls deploy function --function $*",
    "sls-dev": "AWS_PROFILE=${AWS_PROFILE} sls dev --verbose",
    "sls-remove": "AWS_PROFILE=${AWS_PROFILE} sls remove",
    "sls-deploy-local": "AWS_PROFILE=${AWS_PROFILE} sls deploy --stage local",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles",
    "update-packages": "ncu -u"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "@middy/core": "^4.5.5",
    "@middy/error-logger": "^4.6.5",
    "@middy/http-error-handler": "^4.5.5",
    "@middy/http-event-normalizer": "^4.6.5",
    "@middy/http-header-normalizer": "^4.6.5",
    "@middy/http-json-body-parser": "^4.5.5",
    "@middy/http-urlencode-path-parser": "^4.6.5",
    "@middy/validator": "^4.5.5",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "logger": "*",
    "utils": "*"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.370.0",
    "@aws-sdk/credential-providers": "^3.370.0",
    "@aws-sdk/lib-dynamodb": "^3.370.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "esbuild": "<0.18",
    "eslint": "^8.43.0",
    "eslint-config-custom-server": "*",
    "glob": "^10.3.3",
    "jest": "^29.6.1",
    "jest-presets": "*",
    "npm-check-updates": "^16.10.15",
    "run-z": "^1.11.0",
    "serverless": "^3.34.0",
    "serverless-better-credentials": "^1.2.0",
    "serverless-dynamodb": "^0.2.42",
    "serverless-esbuild": "^1.46.0",
    "serverless-localstack": "^1.1.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-webpack": "^5.11.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "fast-xml-parser": ">=4.2.4",
    "semver": ">=7.5.2"
  }
}
