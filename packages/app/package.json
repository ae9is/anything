{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-z clean --then next build",
    "clean": "rm -rf .next dist",
    "dev": "next dev -p 3000",
    "lint": "next lint",
    "start": "next start",
    "update-packages": "ncu -u",
    "s3-empty-bucket": "aws s3 rm s3://${S3_BUCKET:anything-dev-web} --recursive",
    "s3-publish-to-bucket": "aws s3 sync dist s3://${S3_BUCKET:anything-dev-web}",
    "s3-build-and-publish": "run-z build s3-publish-to-bucket"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.414.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.1",
    "@mui/x-data-grid-premium": "^6.10.1",
    "@smithy/signature-v4": "^2.0.7",
    "aws-amplify": "^5.3.4",
    "aws-crt": "^1.15.20",
    "axios": "^1.5.0",
    "formik": "^2.4.2",
    "http-status-codes": "^2.3.0",
    "logger": "*",
    "next": "^13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.7.4",
    "swr": "^2.2.0",
    "utils": "*",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.5.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.2.1",
    "eslint-config-custom": "*",
    "npm-check-updates": "^16.10.15",
    "postcss": "^8.4.26",
    "run-z": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "tsconfig": "*",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.8.0"
  },
  "resolutions": {
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "xlsx": ">=0.19.3"
  },
