{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production run-z clean --then next build",
    "clean": "rm -rf .next dist",
    "dev": "NODE_ENV=dev next dev -p 3000",
    "lint": "next lint",
    "start": "next start",
    "update-packages": "ncu -u",
    "s3-empty-bucket": "aws s3 rm s3://${S3_APP_BUCKET:anything-dev-web} --recursive",
    "s3-publish-to-bucket": "aws s3 sync dist s3://${S3_APP_BUCKET:anything-dev-web}",
    "s3-build-and-publish": "run-z build s3-publish-to-bucket"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^5.2.0",
