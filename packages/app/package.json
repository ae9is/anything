    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/x-data-grid-premium": "^6.16.3",
    "@smithy/signature-v4": "^2.2.0",
    "aws-amplify": "^5.3.11",
    "aws4": "^1.12.0",
    "formik": "^2.4.5",
    "http-status-codes": "^2.3.0",
    "logger": "*",
    "next": "14.1.4",
    "react": "^18.2.0",
    "react-devtools-core": "^5.0.2",
    "react-dom": "^18.2.0",
    "react-select": "^5.8.0",
