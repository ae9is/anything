{
  "name": "infra-data",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "node watch.mjs",
    "build": "tsc",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist .build .esbuild",
    "sls-deploy": "AWS_PROFILE=${AWS_PROFILE} sls deploy",
    "sls-deploy-fn": "AWS_PROFILE=${AWS_PROFILE} sls deploy function --function $*",
    "sls-dev": "AWS_PROFILE=${AWS_PROFILE} sls dev --verbose",
    "sls-remove": "AWS_PROFILE=${AWS_PROFILE} sls remove",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles",
    "update-packages": "ncu -u"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "async": "^3.2.5",
    "aws-kinesis-agg": "^4.2.8",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.540.0",
    "@aws-sdk/credential-providers": "^3.540.0",
    "@aws-sdk/lib-dynamodb": "^3.540.0",
    "@types/async": "^3.2.24",
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "aws-sdk": "^2.1584.0",
    "eslint": "^8.57.0",
    "eslint-config-custom-server": "*",
    "jest": "^29.7.0",
    "jest-presets": "*",
    "npm-check-updates": "^16.14.17",
    "serverless": "^3.38.0",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig": "*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  }
}
